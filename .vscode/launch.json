{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome For React",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}\\packages\\web_react\\src"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Server Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "protocol":"inspector",
            "runtimeExecutable": "/nodejs/npm",
            "runtimeArgs": ["run-script", "start_server_express:dev"]
        }
    ]
}
